{
    "name": "xzxzyzyz/laravel-nova-link-tool",
    "description": "Laravel Nova Link Tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Xzxzyzyz\\LaravelNovaLinkTool\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Xzxzyzyz\\LaravelNovaLinkTool\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
